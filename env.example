# Krib AI Environment Configuration Example
# Copy this file to .env and fill in your actual values

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key  
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Frontend Configuration (Vercel)
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
NEXT_PUBLIC_BACKEND_URL=https://your-app.herokuapp.com/api
NEXT_PUBLIC_URL=https://your-app.vercel.app

# Backend Configuration (Heroku)
DATABASE_URL=your_supabase_database_url
REDIS_URL=redis://localhost:6379

# AI/LLM Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key
OPENAI_API_KEY=your_openai_api_key

# DocuSign Integration
DOCUSIGN_CLIENT_ID=your_docusign_client_id
DOCUSIGN_CLIENT_SECRET=your_docusign_client_secret
DOCUSIGN_ENVIRONMENT=demo # or production

# Payment Processing
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# External APIs
TAVILY_API_KEY=your_tavily_api_key
FIRECRAWL_API_KEY=your_firecrawl_api_key

# Email Configuration
MAILTRAP_API_KEY=your_mailtrap_api_key
MAILTRAP_SENDER_EMAIL=info@krib.ai

# Security
JWT_SECRET=your_jwt_secret
ENCRYPTION_KEY=your_encryption_key 